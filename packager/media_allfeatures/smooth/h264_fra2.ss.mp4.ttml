<head>
	<metadata>
		<title/>
		<desc/>
		<copyright/>
	</metadata>
	<styling>
		<style xml:id="backgroundStyle" fontFamily=" proportionalSansSerif" fontSize="20px" textAlign="center" origin="0% 90%" extent="100% 8%" backgroundColor="red" displayAlign="center"/>
		<style xml:id="speakerStyle" style="backgroundStyle" color="white" textOutline="black 0px 1px" backgroundColor="transparent"/>
   </styling>
   <layout>
		<region xml:id="speaker" style="speakerStyle" zIndex="1"/>
		<region xml:id="background" style="backgroundStyle" zIndex="0"/>
	</layout>
